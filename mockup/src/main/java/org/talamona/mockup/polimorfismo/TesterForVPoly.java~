public class TesterForVPoly {

    public static void main(String[] args) {
        
        F obj = new F();
        
        // METHOD FROM C
        obj.doSomething();
        
        // METHOD FROM E
        obj.doSomething("Luigi");
        
        // METHOD FROM F
        obj.doSomething(true, 0);
    }
}
